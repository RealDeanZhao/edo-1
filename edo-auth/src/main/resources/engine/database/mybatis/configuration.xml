<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <setting name="defaultExecutorType" value="REUSE" />
    </settings>
    <typeAliases>
        <typeAlias type="com.vie.ce.impl.PEScript" alias="Script" />
        <typeAlias type="com.vie.ce.impl.PEAddress" alias="Address" />
        <typeAlias type="com.vie.ce.impl.PERoute" alias="Route" />
        <typeAlias type="com.vie.ce.impl.PERule" alias="Rule" />
        <typeAlias type="com.vie.ce.impl.PEUri" alias="Uri" />
        <typeAlias type="com.vie.ce.impl.PEVerticle" alias="Verticle" />
        <typeAlias type="com.vie.ce.impl.PEField" alias="Field" />
        <typeAlias type="com.vie.ce.impl.PEIndex" alias="Index" />
        <typeAlias type="com.vie.ce.impl.PEKey" alias="Key" />
        <typeAlias type="com.vie.ce.impl.PEMeta" alias="Meta" />
        <typeAlias type="com.vie.ce.impl.PETrigger" alias="Trigger" />
        <typeAlias type="com.vie.ce.impl.PEVColumn" alias="VColumn" />
        <typeAlias type="com.vie.ce.impl.PEView" alias="View" />
        <typeAlias type="com.vie.ce.impl.PESock" alias="Sock" />
    </typeAliases>
    <environments default="H2Environment">
        <environment id="H2Environment">
            <transactionManager type="JDBC" />
            <dataSource type="com.vie.les.mybatis.PEDataSourceFactory" />
        </environment>
    </environments>
    <mappers>
        <mapper resource="database/mybatis/mapper/PEScript.xml" />
        <mapper resource="database/mybatis/mapper/PEAddress.xml" />
        <mapper resource="database/mybatis/mapper/PERoute.xml" />
        <mapper resource="database/mybatis/mapper/PERule.xml" />
        <mapper resource="database/mybatis/mapper/PEUri.xml" />
        <mapper resource="database/mybatis/mapper/PEVerticle.xml" />
        <mapper resource="database/mybatis/mapper/PEMeta.xml" />
        <mapper resource="database/mybatis/mapper/PEKey.xml" />
        <mapper resource="database/mybatis/mapper/PEField.xml" />
        <mapper resource="database/mybatis/mapper/PEView.xml" />
        <mapper resource="database/mybatis/mapper/PEIndex.xml" />
        <mapper resource="database/mybatis/mapper/PEVColumn.xml" />
        <mapper resource="database/mybatis/mapper/PETrigger.xml" />
        <mapper resource="database/mybatis/mapper/PESock.xml" />
    </mappers>
</configuration>